{
  "name": "husky-talisman",
  "version": "3.0.1",
  "description": "Allow running of the ThoughtWorks Talisman tool via node",
  "main": "index.js",
  "scripts": {
    "build": "pack build",
    "publish": "pack publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olmesm/husky-talisman.git"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "git",
    "githooks",
    "hook",
    "hooks",
    "pre-commit",
    "precommit",
    "pre-push",
    "prepush",
    "talisman",
    "husky",
    "lint",
    "ssh keys",
    "authorization tokens",
    "private keys"
  ],
  "author": "Oliver Smit <https://github.com/olmesm>",
  "bugs": {
    "url": "https://github.com/olmesm/husky-talisman/issues"
  },
  "homepage": "https://github.com/olmesm/husky-talisman#readme",
  "license": "MIT",
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-simple-bin": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@pika/types": "^0.9.2",
    "standard": "^12.0.1"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {}
      ],
      [
        "@pika/plugin-build-node",
        {}
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "husky-talisman"
        }
      ]
    ]
  }
}
